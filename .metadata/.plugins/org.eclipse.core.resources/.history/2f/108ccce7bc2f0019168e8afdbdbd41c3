package coding_activity;

import java.io.*;
import java.lang.*;
import java.util.*;
import docs.Jordan;

public class ClassWork {
	
	public static void main(String args[]){
		Scanner scan = new Scanner(System.in);
		boolean same = false;
		double d = 0.0;
		int i = 0;
		d += scan.nextDouble();
		i += scan.nextInt();
		if(d == i){
			say("input int and input double are the same");
			same = true;
		}else if((int) d == i){
			say("input int and input double are very close to each other");
		}else{
			say("input int and input double are not the same");
		}
		say("Same? " + same);
		say("\n Pick a number between 1 and 10");
		int num = scan.nextInt();
		switch(num){
			case 1: say("Let me guess, you typed the number 1? ;o");
			case 2: say("Let me guess, you typed the number 2? ;o");
			case 3: say("Let me guess, you typed the number 3? ;o");
			case 4: say("Let me guess, you typed the number 4? ;o");
			case 5: say("Let me guess, you typed the number 5? ;o");
			case 6: say("Let me guess, you typed the number 6? ;o");
			case 7: say("Let me guess, you typed the number 7? ;o");
			case 8: say("Let me guess, you typed the number 8? ;o");
			case 9: say("Let me guess, you typed the number 9? ;o");
			case 10: say("Let me guess, you typed the number 10? ;o");
		}
	}
	public static void say(Object o){
		Jordan.say(o);
	}
}

