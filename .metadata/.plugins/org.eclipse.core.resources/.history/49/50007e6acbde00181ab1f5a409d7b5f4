    /***************************************************************************************************************/
    
    
    public static void main(String args[]){
        
        // Code goes here..
    	say(recur(9));
    	
    }
    
    public static int recur(int x) {
        if (x >= 0)
        	sau(x);
            return x + recur(x - 1);
        return 0;
    }
    
    
