public class ClassWork extends Jordan {
	
	public static void main(String[] args){
		Scanner scan = new Scanner(System.in);
		boolean flag;
		double d = scan.nextDouble();
		int i = scan.nextInt();
		if(d == i){
			say("d and i are the same");
		}else if((int) d == i){
			say("d and i are very close to each other");
		}else{
			say("d and i are not the same");
		}
	}
}
